{
  "expo": {
    "name": "VietElite Students",
    "slug": "vietelite-parent-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/fav.png",
    "splash": {
      "resizeMode": "contain",
      "image": "./assets/splash.png",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "versionCode": 1,
      "bundleIdentifier": "com.veesoft.vietelitestudents",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "package": "com.veesoft.vietelitestudents",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundColor": "./assets/fav.png"
      }
    },
    "extra": {
      "eas": {
        "projectId": "04b3d310-7a3e-493d-80ea-8917e541fd1e"
      }
    },
    "owner": "tranthanhuet"
  },
  "displayName": "VietElite Students",
  "plugins": [
    // [
    //   "expo-image-picker",
    //   {
    //     "photosPermission": "The app accesses your photos to let you share them with your friends."
    //   }
    // ],
    [
      "expo-notifications",
      {
        "icon": "./assets/logo.png",
        "color": "#ffffff"
      }
    ]
  ]
}
